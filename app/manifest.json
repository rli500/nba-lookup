{
  "name": "NBA Quick Stats",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Get quick stats on any NBA Player!",
  "permissions": [
    "tabs",
    "contextMenus",
    "https://us-central1-nbanba.cloudfunctions.net/main"
  ],
  "icons": {
    "32": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup/default.html",
    "default_title": "Description of App."
  }
}